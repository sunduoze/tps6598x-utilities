def altmodeQueueSVID1(argumentList) :
    if (argumentList[0].valueToInt() == 1) and (argumentList[1].valueToInt() == 1)  :
        DPIsSet = 1
    else :
        DPIsSet = 0

    if (argumentList[2].valueToInt() == 1) and (argumentList[3].valueToInt() == 1)  :
        TBTIsSet = 1
    else :
        TBTIsSet = 0

    if TBTIsSet == 1 :
        return 0x8087
    elif DPIsSet == 1 :
        return 0xFF01
    else :
        return 0


