def adjPSDerivedFxn(argumentList) :
    if (argumentList[0].valueToInt() == 1) and (argumentList[1].valueToInt() == 1) :
        isSet = 1
    else :
        isSet = 0

    # force consistency
    argumentList[0].setFromInt(isSet)
    argumentList[1].setFromInt(isSet)

    return isSet

